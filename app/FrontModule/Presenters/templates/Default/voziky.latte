                    <img class="carousel-cell" src="{$basePath}/assets/img/towpoint-vozik.jpg?" alt="Volne stojici vozik">
                    <img class="carousel-cell" src="{$basePath}/assets/img/towpoint-upinaky.jpg?" alt="Upinaci pasy">
                    <img class="carousel-cell" src="{$basePath}/assets/img/towpoint-dilna-ruka.jpg?" alt="Spoluprace na dilne">
                    <img class="carousel-cell" src="{$basePath}/assets/img/towpoint-vozik-za-autem.jpg" alt="Vyber voziku">
                {/cache}
            </div>
        </section>
    </footer>

